<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<servlet>
		<servlet-name>EaTime</servlet-name>
		<servlet-class>com.bingo.eatime.EaTimeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EaTimeLogin</servlet-name>
		<servlet-class>com.bingo.eatime.EaTimeLoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EaTimeLogOut</servlet-name>
		<servlet-class>com.bingo.eatime.EaTimeLogOut</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EaTimeDatabaseDemo</servlet-name>
		<servlet-class>com.bingo.eatime.test.EaTimeDatabaseDemoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EaTimeDatabaseTest</servlet-name>
		<servlet-class>com.bingo.eatime.test.EaTimeDatabaseTestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DatabaseForFrontEndTest</servlet-name>
		<servlet-class>com.bingo.eatime.test.DatabaseForFrontEndTest</servlet-class>
	</servlet>
	<servlet>
	    <servlet-name>EventAdminServlet</servlet-name>
	    <servlet-class>com.bingo.eatime.admin.EventAdminServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EaTime</servlet-name>
		<url-pattern>/eatime</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EaTimeLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EaTimeLogOut</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EaTimeDatabaseDemo</servlet-name>
		<url-pattern>/test/demo</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EaTimeDatabaseTest</servlet-name>
		<url-pattern>/test/db</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DatabaseForFrontEndTest</servlet-name>
		<url-pattern>/test/ryan</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	    <servlet-name>EventAdminServlet</servlet-name>
	    <url-pattern>/event</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>eatime</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>

</web-app>